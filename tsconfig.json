{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "modules/*": ["./modules/*"],
      "routes/*": ["features/*"],
      "sql": ["./sql"],
      "config": ["./config"]
    }
  }
}